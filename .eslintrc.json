{
  "extends": ["react-app", "react-app/jest"],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn",
    "prefer-const": "error",
    "no-var": "error"
  },
  "overrides": [
    {
      "files": ["**/*.test.js", "**/*.test.tsx", "src/setupTests.js"],
      "rules": {
        "testing-library/no-container": "off",
        "testing-library/no-node-access": "off",
        "no-console": "off"
      }
    }
  ]
}